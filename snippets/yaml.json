{
  "AppVeyor": {
    "prefix": "ci-appveyor",
    "description": "AppVeyor configuration file",
    "body": [
      "environment:",
      "\tnodejs_version: \"6\"",
      "",
      "install:",
      "\t- ps: Install-Product node $env:nodejs_version",
      "\t- npm install",
      "",
      "test_script:",
      "\t- node --version",
      "\t- npm --version",
      "\t- npm test",
      "",
      "build: off",
      "$0"
    ]
  },
  "Circle": {
    "prefix": "ci-circle",
    "description": "Circle configuration file",
    "body": [
      "machine:",
      "\tnode:",
      "\t\tversion: 6.10.3",
      "",
      "dependencies:",
      "\toverride:",
      "\t\t- npm install",
      "",
      "test:",
      "\toverride:",
      "\t\t- npm test",
      "$0"
    ]
  },
  "Travis": {
    "prefix": "ci-travis",
    "description": "Travis configuration file",
    "body": [
      "language: node_js",
      "",
      "node_js:",
      "\t-\"6\"",
      "",
      "install:",
      "\t- npm install",
      "",
      "script:",
      "\t- npm test",
      "",
      "after_script:",
      "\t- npm run coveralls",
      "",
      "notifications:",
      "\temail:",
      "\t\ton_success: never",
      "\t\ton_failure: always",
      "$0"
    ]
  }
}
