{
  "ESLint": {
    "prefix": "eslintrc",
    "description": "ESLint file",
    "body": [
      "{",
      "\t\"env\": {",
      "\t\t\"es6\": true,",
      "\t\t\"jest\": true,",
      "\t\t\"node\": true",
      "\t},",
      "\t\"extends\": \"eslint:recommended\",",
      "\t\"rules\": {",
      "\t\t\"indent\": [",
      "\t\t\t\"error\",",
      "\t\t\t2",
      "\t\t],",
      "\t\t\"linebreak-style\": [",
      "\t\t\t\"error\",",
      "\t\t\t\"unix\"",
      "\t\t],",
      "\t\t\"quotes\": [",
      "\t\t\t\"error\",",
      "\t\t\t\"single\"",
      "\t\t],",
      "\t\t\"semi\": [",
      "\t\t\t\"error\",",
      "\t\t\t\"always\"",
      "\t\t]",
      "\t}",
      "}",
      "$0"
    ]
  },
  "package.json": {
    "prefix": "package",
    "description": "package.json file",
    "body": [
      "{",
      "\t\"name\": \"${1:project-name}\",",
      "\t\"version\": \"${2:1.0.0}\",",
      "\t\"description\": \"${1:project-name} is an awesome project.\",",
      "\t\"author\": \"${3:author-name} <${4:email}>\",",
      "\t\"homepage\": \"https://github.com/${5:github-username}/${1:project-name}#readme\",",
      "\t\"keywords\": [",
      "\t\t\"${6:keyword}\"",
      "\t],",
      "\t\"main\": \"${7:index.js}\",",
      "\t\"scripts\": {",
      "\t\t\"start\": \"${8:start}\",",
      "\t\t\"test\": \"${9:test}\"",
      "\t},",
      "\t\"repository\": {",
      "\t\t\"type\": \"git\",",
      "\t\t\"url\": \"https://github.com/${5:github-username}/${1:project-name}.git\"",
      "\t},",
      "\t\"bugs\": {",
      "\t\t\"url\": \"https://github.com/${5:github-username}/${1:project-name}/issues\"",
      "\t},",
      "\t\"dependencies\": {},",
      "\t\"devDependencies\": {},",
      "\t\"private\": true,",
      "\t\"license\": \"${10:MIT}\"",
      "}",
      "$0"
    ]
  }
}
